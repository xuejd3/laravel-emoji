{
    "name": "xuejd3/laravel-emoji",
    "description": "An emojione bridge for Laravel",
    "type": "library",
    "require": {
        "joypixels/emoji-toolkit": "^9.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "xuejd3",
            "email": "xuejd3@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Xuejd3\\LaravelEmoji\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xuejd3\\LaravelEmoji\\EmojiServiceProvider"
            ],
            "aliases": {
                "Emoji": "Xuejd3\\LaravelEmoji\\Emoji"
            }
        }
    },
    "require-dev": {
        "laravel/pint": "^1.22"
    }
}
